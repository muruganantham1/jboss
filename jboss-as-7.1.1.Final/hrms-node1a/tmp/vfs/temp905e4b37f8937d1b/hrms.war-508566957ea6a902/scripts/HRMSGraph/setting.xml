<settings>
  <thousands_separator>,</thousands_separator>
  <decimals_separator>.</decimals_separator>
  <digits_after_decimal>2</digits_after_decimal>
  <text_size>10</text_size>
  <background>
    <alpha>100</alpha>
  </background>
  <grid>
    <category>
      <alpha>41</alpha>
      <dashed>1</dashed>
    </category>
    <value>
      <alpha>39</alpha>
      <dashed>1</dashed>
      <fill_alpha>20</fill_alpha>
    </value>
  </grid>
  <axes>
    <category>
      <width>1</width>
      <color>E7E7E7</color>
    </category>
    <value>
      <width>1</width>
      <color>E7E7E7</color>
    </value>
  </axes>
  <values>
    <value>
      <min>0</min>
    </value>
  </values>
  <depth>15</depth>
  <column>
    <width>60</width>
    <balloon_text>{title}: {value}</balloon_text>
    <grow_time>3</grow_time>
  </column>
  <strings>
    <no_data>There are no reports for you at this time</no_data>
  </strings>
  <graphs>
    <graph gid="1">
      <title>Total Salary</title>
      <color>0099CC</color>
    </graph>
  </graphs>
</settings>